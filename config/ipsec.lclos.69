/etc/ipsec.conf


# ===========================
# StrongSwan - Branch 1
# ===========================

config setup
    charondebug="ike 1, knl 1, cfg 0"
    uniqueids=no

conn HQ
    auto=start
    type=tunnel
    keyexchange=ikev2
    left=10.10.10.2
    leftid=@Branch10
    leftauth=psk
    right=10.10.10.1
    rightid=@HQ10
    rightauth=psk
    ike=aes128-sha256-modp2048
    esp=aes128-sha256
    lifetime=45m




/etc/ipsec.secrets

@Branch10 @HQ10 : PSK "network"


