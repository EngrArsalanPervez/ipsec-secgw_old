/etc/ipsec.conf

conn HQ
    auto=start
    type=tunnel
    keyexchange=ikev2
    left=10.10.10.2
    leftid=@Branch
    leftauth=psk
    right=10.10.10.1
    rightid=@HQ
    rightauth=psk
    ike=aes128-sha256-modp2048
    esp=aes128-sha256
    lifetime=45m


/etc/ipsec.secrets

@Branch @HQ : PSK "network"
